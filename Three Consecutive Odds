class Solution:
    def threeConsecutiveOdds(self, arr: List[int]) -> bool:
        count = 0
        for i in range(len(arr)):
            if count == 3:
                break
            elif arr[i] % 2 == 1:
                count += 1
            else:
                print(arr[i])
                count = 0
        if count == 3:
            return True
        else:
            return False
